        "start-no-decorator": "node ./src/build/scripts/start-dev-nodecorator.js",
        "precommit": "pretty-quick --staged",
        "jest": "jest --config ./jest/jest.config.js",
        "jest-watch": "jest --watch --verbose --config ./jest/jest.config.js"
    },
