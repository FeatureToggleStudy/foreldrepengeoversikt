    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.5.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "path": "^0.12.7",
    "prettier": "1.12.1",
